[user]
  name = Calvin Cieslak
  email = calvin.cieslak@gmail.com
[core]
  editor = vim
  excludesfile = /Users/Calvin/.gitignore_global
[color]
  ui = true
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[color "diff"]
  meta = 11
  frag = magenta bold
  commit = yellow bold
  old = red bold
  new = green bold
  whitespace = red reverse
[init]
	defaultBranch = main
