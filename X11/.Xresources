Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! urxvt
URxvt.scrollBar: false
URxvt.font: xft:Meslo LG M:size=10:antialias=true,xft:TerminessTTF Nerd Font Mono:size=14
URxvt.perl-ext-common: default,font-size,url-select,selection-to-clipboard
URxvt.letterSpace: -1
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.internalBorder: 10
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard

!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36

! Black
*.color0:     #000000
*.color8:     #4D4D4D

! Red
*.color1:     #FF5555
*.color9:     #FF6E67

! Green
*.color2:     #50FA7B
*.color10:    #5AF78E

! Yellow
*.color3:     #F1FA8C
*.color11:    #F4F99D

! Magenta
*.color4:     #BD93F9
*.color12:    #CAA9FA

! Pink
*.color5:     #FF79C6
*.color13:    #FF92D0

! Teal
*.color6:     #8BE9FD
*.color14:    #9AEDFE

! Gray
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! Colors states:   'bg', 'fg', 'bgalt', 'hlbg', 'hlfg'
rofi.color-normal: #282A36, #F8F8F2, #282A36, #424450, #F8F8F2
rofi.color-active: #282A36, #F8F8F2, #282A36, #424450, #F8F8F2
!                  'background', 'border', 'separator'
rofi.color-window: #282A36, #CAA9FA, #282A36

! vim:syntax=xdefaults
