Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! urxvt
URxvt.scrollBar: false
URxvt.font: xft:Meslo LG M:size=10:antialias=true,xft:TerminessTTF Nerd Font Mono:size=14
URxvt.perl-ext-common: default,font-size,url-select,selection-to-clipboard
URxvt.letterSpace: -1
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

! Prevent ISO notification when attempting to copy/paste
URxvt.iso14755: False

*.foreground:   #D8DEE9
*.background:   #252A33
*.cursorColor:  #D8DEE9

*.color0: #3B4252
*.color1: #BF616A
